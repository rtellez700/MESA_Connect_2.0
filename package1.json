    "compression": "^1.6.1",
    "events": "^1.1.0",
    "express": "^4.13.4",
    "flux": "^2.1.1",
    "grunt": "^0.4.5",
    "imports-loader": "^0.6.5",
    "jquery": "^2.2.1",
    "keymirror": "^0.1.1",
    "node-jsx": "^0.13.3",
