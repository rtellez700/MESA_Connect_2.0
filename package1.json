{
  "name": "mesa_connect_2.0",
  "version": "0.0.1",
  "description": "MESA_Connect Application",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rtellez700/MESA_Connect_2.0"
  },
  "scripts": {
    "start": "if-env cross-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "gulp && webpack-dev-server -d --colors --port 3030 --inline --content-base public/ --history-api-fallback",
    "start:prod": "npm run build && node server.bundle.js",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server && gulp"
  },
